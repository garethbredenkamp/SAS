program SAS;

uses
  Vcl.Forms,
  uLogin in 'uLogin.pas' {gLogin},
  uDash in 'uDash.pas' {gDashboard};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TgLogin, gLogin);
  Application.CreateForm(TgDashboard, gDashboard);
  Application.Run;
end.
